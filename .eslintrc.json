{
	"extends": "./node_modules/@theintern/dev/eslint.config.js",
	"rules": {
		"@typescript-eslint/no-empty-function": "off",
		"@typescript-eslint/no-floating-promises": "off",
		"@typescript-eslint/no-implied-eval": "off",
